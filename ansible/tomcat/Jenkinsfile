pipeline {
  agent any
  
  stages {
    stage('checkout stage') {
      steps {
        sh 'rm -rf tomcatt'
        sh 'git clone https://github.com/yatheesh2328/Yatish_Repository/ansible/tomcat.git'
      }
    }
    stage('running playbook') {
      steps {
        sh 'ansible-playbook -i hosts tomcat.yml'
      }
    }
  }
}
